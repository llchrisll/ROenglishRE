function GetValue(valueName)
  return _G[valueName]
end
function IsExistValue(valueName)
  local value = _G[valueName]
  if value ~= nil then
    return true
  end
  return false
end
function IsOpenedEmitterInfoFiles(InfoTableName)
  local EmitterInfoTable = _G[InfoTableName]
  if EmitterInfoTable ~= nil then
    return true
  end
  return false
end
